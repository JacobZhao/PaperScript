@article{Humada2014,
abstract = {Now a day's solar photovoltaic energy played a wider role in terms of electrical power energy generation. This type of power energy can face many obstacles; like partial shadow conditions, aging PV cells and modules, etc. these difficulties can be avoided by many ways like the operation of reconfigurable of photovoltaic (PV) systems. The reconfiguration is of great interest with respect to system designers in order to improve the system's efficiency and operation. This paper suggests a new method to present an algorithm of reconfiguration PV system strategy under partially shaded conditions (PSCs), for central converter photovoltaic (PV) systems, is introduced. The technique of reconfiguration the photovoltaic (PV) system accomplished with the help of DC/DC central converter. In the next step, an accurate arrangement and connection of the modules that have the same value of irradiation level assembled in one group string. Then theses strings with different irradiation levels, each connect to DC-DC converter. Suggested algorithm increases the power of the solar PV array according to a model-based algorithm. The model algorithms are implemented in real time. Finally, the proposed solution is ensuring that the adaptive string has the ability to capture the MPP. Keywords:},
author = {Humada, A M and Hojabri, M and Mohamed, M B and Ahmed, M N and Sulaiman, M H Bin},
file = {:home/dafang-z/Papers/International Journal of Renewable Energy Resources 4 (2014)/Humada et al/Humada et al. - 2014 - a New Method of Pv Reconfiguration Under Partial Shadow Conditions Based on Dc Dc Central Converter.pdf:pdf},
journal = {International Journal of Renewable Energy Resources 4 (2014)},
keywords = {Maximum power point (MPP).,PV Efficiency,Partial shadow condition,Photovoltaic (PV) system},
pages = {49--53},
title = {{a New Method of Pv Reconfiguration Under Partial Shadow Conditions Based on Dc / Dc Central Converter}},
volume = {4},
year = {2014}
}
