file = {:home/dafang-z/Papers/IEEE Transactions on Energy Conversion/Chen, Rincon-Mora/Chen, Rincon-Mora - 2006 - Accurate Electrical Battery Model Capable of Predicting Runtime and I–V Performance.pdf:pdf},
issn = {0885-8969},
journal = {IEEE Transactions on Energy Conversion},
keywords = {Colon,Interleukin-10,Interleukin-2,Interleukin-6,Nicotine,Small bowel},
month = {jun},
number = {2},
pages = {504--511},
title = {{Accurate Electrical Battery Model Capable of Predicting Runtime and I–V Performance}},
