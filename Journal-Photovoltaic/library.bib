keywords = {Battery ageing,Battery modelling,Degradation,Grid-connected battery,Lithium ion,Optimal control},
number = {December 2017},
pages = {91--102},
publisher = {Elsevier},
title = {{Improving optimal control of grid-connected lithium-ion batteries through more accurate battery and degradation modelling}},
url = {https://doi.org/10.1016/j.jpowsour.2018.01.004},
volume = {379},
year = {2018}
}
