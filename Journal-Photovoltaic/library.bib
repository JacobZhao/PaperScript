issn = {18790682},
journal = {Renewable Energy},
keywords = {Fault detection,MATLAB/Simulink,Multiple PV array configurations,Partial shading},
pages = {438--460},
publisher = {Elsevier Ltd},
title = {{Seven indicators variations for multiple PV array configurations under partial shading and faulty PV conditions}},
url = {http://dx.doi.org/10.1016/j.renene.2017.06.014},
volume = {113},
year = {2017}
}
