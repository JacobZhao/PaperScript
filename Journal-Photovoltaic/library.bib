isbn = {1949-3029},
issn = {19493029},
journal = {IEEE Transactions on Sustainable Energy},
keywords = {Explicit model,Lambert W function,Maximum power point (MPP),Partial shading,Photovoltaic (PV) string,Simplified formulae},
