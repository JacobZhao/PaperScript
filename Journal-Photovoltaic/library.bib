journal = {International Journal of Electrical Power and Energy Systems},
keywords = {Maximum power point,PV array reconfiguration,Partial shaded conditions,Photovoltaic arrays},
pages = {713--721},
title = {{PV array reconfiguration method under partial shading conditions}},
volume = {63},
year = {2014}
}
